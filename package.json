{
  "name": "@infect/beta-ship",
  "version": "1.0.0",
  "description": "main application for rda",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "app": "node --max-old-space-size=2000 --experimental-modules --no-warnings --experimental-vm-modules index.js --dev --log-level=error+ --log-module=* --debug-children",
    "import": "node --max-old-space-size=16000 --experimental-modules --no-warnings --experimental-vm-modules src/import.js --dev --log-level=error+ --log-module=*",
    "cluster": "node --max-old-space-size=16000 --experimental-modules --no-warnings --experimental-vm-modules src/createCluster.js --dev --log-level=error+ --log-module=*",
    "app-int": "node --max-old-space-size=2000 --experimental-modules --no-warnings --experimental-vm-modules index.integration.js --integration --log-level=error+ --log-module=*",
    "cluster-int": "node --max-old-space-size=16000 --experimental-modules --no-warnings --experimental-vm-modules src/createCluster.js --integration --log-level=error+ --log-module=*",
    "link": "node --experimental-modules --no-warnings ./node_modules/.bin/linker --dev --l",
    "update": "node --experimental-modules --no-warnings ./node_modules/.bin/updater --l --dev --group=rda-service"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infect.org/beta-ship.git"
  },
  "keywords": [
    "rda",
    "infect"
  ],
  "author": "Lina van der Weg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/infect.org/beta-ship/issues"
  },
  "homepage": "https://github.com/infect.org/beta-ship#readme",
  "dependencies": {
    "@distributed-systems/http2-client": "github:distributed-systems/http2-client",
    "@infect/api": "^2.0.3",
    "@infect/infect-rda-sample-importer": "^2.0.0",
    "@infect/infect-rda-sample-storage": "^3.0.1",
    "@infect/rda": "^2.0.0",
    "@infect/rda-cluster-service": "^2.0.0",
    "@infect/rda-compute-service": "^2.0.1",
    "@infect/rda-coordinator-service": "^2.0.0",
    "@infect/rda-lock-service": "^2.0.0",
    "@infect/rda-service-manager": "^2.0.4",
    "@infect/rda-service-registry": "^2.0.3",
    "@infect/rda-service-registry-client": "^2.1.1",
    "@joinbox/eslint-config-joinbox": "^1.0.8",
    "csv": "^5.1.3",
    "depstr": "^1.1.1",
    "ee-log": "^3.0.9",
    "fs-promise": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^5.16.0"
  },
  "type": "module"
}
